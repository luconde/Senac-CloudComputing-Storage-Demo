@model IEnumerable<DStorageV2.Library.FileEntity>

@{
    ViewData["Title"] = "Listar";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Listar</h1>

<table class="table">
    <thead>
        <tr>
            <th>
                Aluno
            </th>
            <th>
                Identificar único do arquivo
            </th>
            <th>
                Nome do arquivo
            </th>
            <th>
                Provedor de destino
            </th>
            <th>
                Data & Hora
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.PartitionKey)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RowKey)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FileName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Destino)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Timestamp)
            </td>
        </tr>
}
    </tbody>
</table>
